        <Rectangle Grid.Column="3" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="397" Margin="440,0,0,0" Grid.Row="1" Stroke="Black" VerticalAlignment="Top" Width="234" Fill="#FFF5EB7E"/>
