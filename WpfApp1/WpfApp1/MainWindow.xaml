        <Button x:Name="BAddLivingRoom" Grid.Column="3" Content="Добавить" HorizontalAlignment="Left" Height="42" Margin="92,402,0,0" Grid.Row="1" VerticalAlignment="Top" Width="156" Click="BAddLivingRoom_Click" FontWeight="Bold" FontSize="14"/>
        <Button x:Name="BAddOffice" Grid.Column="3" Content="Добавить" HorizontalAlignment="Left" Height="42" Margin="272,402,0,0" Grid.Row="1" VerticalAlignment="Top" Width="156" Click="BAddOffice_Click" FontWeight="Bold" FontSize="14"/>
        <Button x:Name="BGetList" Grid.Column="3" Content="Выдать" Grid.ColumnSpan="2" HorizontalAlignment="Right" Height="42" Margin="0,402,51,0" Grid.Row="1" VerticalAlignment="Top" Width="156" Click="BGetList_Click" FontWeight="Bold" FontSize="14"/>
