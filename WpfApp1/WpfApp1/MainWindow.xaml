<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Окно" Width="800" Height="499" MaxWidth="800" MaxHeight="499" ScrollViewer.VerticalScrollBarVisibility="Disabled" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,0,-6">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="16*"/>
            <ColumnDefinition Width="15*"/>
            <ColumnDefinition Width="83*"/>
            <ColumnDefinition Width="485*"/>
            <ColumnDefinition Width="201*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="11*"/>
            <RowDefinition Height="423*"/>
        </Grid.RowDefinitions>
        <Rectangle Grid.ColumnSpan="4" HorizontalAlignment="Left" Height="397" Grid.Row="1" Stroke="#FF060606" VerticalAlignment="Top" Width="180" Fill="#FF73E8BA" Margin="14,0,0,0"/>
        <Rectangle Grid.Column="3" HorizontalAlignment="Left" Height="397" Grid.Row="1" Stroke="#FF060606" VerticalAlignment="Top" Width="180" Fill="#FFDCA6F7" Margin="80,0,0,0"/>
