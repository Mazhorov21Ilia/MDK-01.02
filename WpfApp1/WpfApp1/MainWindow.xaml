        <Label Grid.Column="3" Content="Жилое помещение" HorizontalAlignment="Left" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="157" FontSize="14" FontWeight="Bold" Margin="85,0,0,0"/>
        <Label Grid.Column="3" Content="Офис" HorizontalAlignment="Left" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="98" FontSize="14" FontWeight="Bold" Margin="265,0,0,0"/>
        <Label Grid.Column="3" Content="Список помещений" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="165" FontSize="14" FontWeight="Bold" Margin="445,0,0,0"/>
        <Label Grid.Column="1" Content="Длина" Grid.ColumnSpan="2" HorizontalAlignment="Center" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="98" FontSize="14" Margin="0,58,0,0"/>
        <Label Grid.Column="1" Content="Ширина" Grid.ColumnSpan="2" HorizontalAlignment="Center" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="98" FontSize="14" Margin="0,109,0,0"/>
        <Label Grid.Column="3" Content="Длина" HorizontalAlignment="Left" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="98" FontSize="14" Margin="80,58,0,0"/>
        <Label Grid.Column="3" Content="Длина" HorizontalAlignment="Left" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="98" FontSize="14" Margin="260,58,0,0"/>
        <Label Grid.Column="3" Content="Ширина" HorizontalAlignment="Left" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="98" FontSize="14" Margin="80,109,0,0"/>
        <Label Grid.Column="3" Content="Ширина" HorizontalAlignment="Left" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="98" FontSize="14" Margin="260,109,0,0"/>
        <Label Grid.Column="3" Content="Число окон" HorizontalAlignment="Left" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="98" FontSize="14" Margin="80,171,0,0"/>
        <Label Grid.Column="3" Content="Число розеток" HorizontalAlignment="Left" Height="27" Grid.Row="1" VerticalAlignment="Top" Width="116" FontSize="14" Margin="256,171,0,0"/>
        <TextBox x:Name="TBLenghtR" Grid.Column="3" HorizontalAlignment="Left" Margin="0,58,0,0" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" Height="36" TextChanged="TBLenghtR_TextChanged"/>
        <TextBox x:Name="TBWidthR" Grid.Column="3" HorizontalAlignment="Left" Margin="0,105,0,0" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" Height="36" TextChanged="TBWidthR_TextChanged"/>
        <TextBox x:Name="TBLengthL" Grid.Column="3" HorizontalAlignment="Left" Margin="179,58,0,0" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" Height="36"/>
