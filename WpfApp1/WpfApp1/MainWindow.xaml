        <Button x:Name="BGetList_Copy" Grid.Column="4" Content="Очистить" HorizontalAlignment="Right" Height="32" Margin="0,360,78,0" Grid.Row="1" VerticalAlignment="Top" Width="98" Click="BGetList_Copy_Click" FontWeight="Bold" FontSize="14"/>
        <Button x:Name="BGetList_Copy1" Grid.Column="2" Content="Очистить" HorizontalAlignment="Right" Height="32" Margin="0,360,445,0" Grid.Row="1" VerticalAlignment="Top" Width="98" Click="BGetList_Copy1_Click" FontWeight="Bold" FontSize="14" Grid.ColumnSpan="2"/>
        <Button x:Name="BGetList_Copy2" Grid.Column="3" Content="Очистить" HorizontalAlignment="Right" Height="32" Margin="0,360,86,0" Grid.Row="1" VerticalAlignment="Top" Width="98" Click="BGetList_Copy2_Click" FontWeight="Bold" FontSize="14"/>
        <Button x:Name="BGetList_Copy3" Grid.Column="3" Content="Очистить" HorizontalAlignment="Right" Height="32" Margin="0,360,265,0" Grid.Row="1" VerticalAlignment="Top" Width="98" Click="BGetList_Copy3_Click" FontWeight="Bold" FontSize="14"/>

